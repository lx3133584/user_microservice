{
  "compilerOptions": {
    "module": "commonjs",
    //"declaration": true,
    "removeComments": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "target": "es6",
    "sourceMap": true,
    "outDir": "./dist",
    //"baseUrl": ".",
    "strict": true,
    "strictPropertyInitialization": false,
    "skipLibCheck": true,
    "suppressImplicitAnyIndexErrors": true
  },
  "exclude": ["node_modules", "dist"]
}
